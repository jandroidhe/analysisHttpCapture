      attribute 225666 "SSL_CONTEXT_PROTOCOL"
	class_attribute const_attribute private explicit_type "String"
	init_value " \"TLSv1.2\""
	cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
